{
  "name": "@rxcc/debug",
  "version": "0.1.0",
  "description": "utility functions for debugging component combinators",
  "repository": {
    "type": "git",
    "url": "https://github.com/brucou/component-combinators"
  },
  "keywords": [
    "reactive",
    "rxjs",
    "componentization",
    "components",
    "functional programming",
    "debug"
  ],
  "main": "dist-node/index.js",
  "module": "src/index.js",
  "jsnext:main": "src/index.js",
  "files": [
    "lib/",
    "src/"
  ],
  "scripts": {
    "build-test": "babel test --source-maps --out-dir dist-node-test",
    "test": "watchify -d --transform babelify dist-node-test -o test/bundle.js -v",
    "wbuild": "webpack --display-error-details --progress --colors --watch --env dev",
    "wserve": "webpack-dev-server --content-base=./ --hot --inline --watch",
    "wrelease": "webpack --display-error-details --progress --colors --env build",
    "rollup": "./node_modules/.bin/rollup -c",
    "wrollup": "./node_modules/.bin/rollup -c --environment NODE_ENV:production"
  },
  "engines": {
    "node": "8.2.0"
  },
  "author": "brucou",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.9.1",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.5.0",
    "browserify": "^11.2.0",
    "css-loader": "^0.24.0",
    "del": "^2.2.0",
    "derequire": "^2.0.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "image-webpack-loader": "^2.0.0",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.5.3",
    "nodemon": "^1.9.2",
    "postcss-loader": "^0.11.0",
    "rimraf": "^2.4.3",
    "rollup": "^0.54.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "run-sequence": "^1.1.5",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.13.1",
    "surge": "^0.17.7",
    "uglify-js": "^2.5.0",
    "webpack": "3.1.0",
    "webpack-dev-server": "^2.6.1",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-stream": "^3.1.0",
    "yargs": "7.1.0"
